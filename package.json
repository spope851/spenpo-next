{
  "name": "spenpo-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "codegen": "graphql-codegen --config codegen.ts",
    "start:server": "cd server && docker-compose -p spenpo up -d",
    "new:db": "cd server && rm -rf postgres-data && docker-compose build && docker-compose up",
    "it:db": "docker exec -it db sh",
    "it:cache": "docker exec -it redis redis-cli",
    "stripe": "stripe listen --forward-to localhost:3000/api/stripe-hooks",
    "stripe:rc": "stripe listen --forward-to https://spenpo-next-git-feature-landing-spope851.vercel.app/api/stripe-hooks",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "3x3-cube": "^2.1.3",
    "@dhaiwat10/react-link-preview": "^1.15.0",
    "@emailjs/browser": "^3.10.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.11.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/font": "13.1.1",
    "@prisma/client": "^5.1.1",
    "@stripe/react-stripe-js": "^2.1.2",
    "@stripe/stripe-js": "^2.1.0",
    "@svgr/webpack": "^8.1.0",
    "animate.css": "^4.1.1",
    "argon2": "^0.31.0",
    "aws-sdk": "^2.1445.0",
    "class-validator": "^0.14.0",
    "cubejs": "^1.3.2",
    "html-metadata-parser": "^2.0.4",
    "ioredis": "^5.3.1",
    "lodash": "^4.17.21",
    "micro": "^10.0.1",
    "next": "13.1.1",
    "next-auth": "^4.22.4",
    "nodemailer": "^6.9.4",
    "octokit": "^3.1.0",
    "pg": "^8.8.0",
    "react": "^18",
    "react-color": "^2.19.3",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-player": "^2.14.1",
    "react-social-icons": "^5.15.0",
    "react-timeago": "^7.1.0",
    "react-timeclock": "^0.2.0",
    "react-timer-hook": "^3.0.7",
    "spenpo-svelte": "./svelte-apps",
    "stripe": "^13.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18",
    "@types/react-timeago": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^2.8.2",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
