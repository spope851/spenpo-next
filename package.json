    "stripe": "stripe listen --forward-to localhost:3000/api/stripe-hooks",
    "stripe:rc": "stripe listen --forward-to https://spenpo-next-git-feature-landing-spope851.vercel.app/api/stripe-hooks",
    "postinstall": "prisma generate --no-engine"
